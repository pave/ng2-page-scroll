{
  "name": "ng2-page-scroll",
  "version": "0.2.3",
  "description": "Pure angular2 directive for scrolling to html anchors",
  "scripts": {
    "install:typings": "./node_modules/.bin/typings install",
    "compile": "./node_modules/.bin/tsc",
    "prepublish": "npm run install:typings && ./node_modules/.bin/tsc && node ./make.js"
  },
  "main": "ng2-page-scroll.js",
  "typings": "ng2-page-scroll.d.ts",
  "keywords": [
    "angular2",
    "angularjs",
    "page",
    "scroll",
    "pagescroll"
  ],
